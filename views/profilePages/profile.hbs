<h1>Profile Page</h1>
<a href="/logout">Log out</a>
<div class='profile-info'>
  <h2>Hey {{user.username}}, Welcome to your profile</h2>
  <h3> {{user.email}}</h3>
  <img src="{{user.photo}}" alt="user photo" />
  <p>{{user.country}}</p>
  <a href='/profile/{{user._id}}/edit'>
    <button class="btn">Edit your profile</button>
  </a>

  <ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item" role="presentation">
      <button class="nav-link active" id="home-tab" data-bs-toggle="tab" data-bs-target="#Wishlist" type="button" role="tab"
        aria-controls="home" aria-selected="true">Wishlist</button>
    </li>
    <li class="nav-item" role="presentation">
      <button class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#profile" type="button" role="tab"
        aria-controls="profile" aria-selected="false">Already Visited countries</button>
    </li>
  </ul>
  <div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">Countries</div>
    <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">Test</div>
  </div>
</div>