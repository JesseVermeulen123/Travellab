<div id="catchPhrase">
  <h1>Discover more about {{data.name}}</h1>
</div>
<div class="container">
  <div class="row align-items-start">
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Native</h6>
      <div id="solidBorder"></div>{{data.nativeName}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Capital</h6>
      <div id="solidBorder"></div> {{data.capital}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Code</h6>
      <div id="solidBorder"></div>{{data.callingCodes}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Continent</h6>
      <div id="solidBorder"></div>{{data.region}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Subregion</h6>
      <div id="solidBorder"></div>{{data.subregion}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Population</h6>
      <div id="solidBorder"></div>{{data.population}} people</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Population name</h6>
      <div id="solidBorder"></div>{{data.demonym}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Timezone</h6>
      <div id="solidBorder"></div>{{#each data}}{{this.timezones}}{{/each}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Borders</h6>
      <div id="solidBorder"></div>{{#each data}}{{this.borders}}{{/each}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Currencies</h6>
      <div id="solidBorder"></div>{{#each data}}{{#each this.currencies}}
      <p>{{this.name}}</p>
      <p>{{this.symbol}}</p>{{/each}}{{/each}}</p>
    </div>
    <div class="col borderCountry darkmode-ignore">
      <p>
      <h6>Languages</h6>
      <div id="solidBorder"></div>{{#each data}}{{#each this.languages}}
      <p>{{this.name}}</p>{{/each}} {{/each}}</p>
    </div>
  </div>
</div>

<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner  carousel_size">
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <div class="carousel-item active">
      <img class="d-block w-100" src="{{data.flag}}" alt="{{data.name}} flag">
    </div>
    {{#each images}}
    <div class="carousel-item">
      <img class="d-block w-100" src={{this.urls.regular}} alt="{{this.alt_description}}">
    </div>
    {{/each}}
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>


<div class="buttonProfile">
    <a href="/country/{{data._id}}/countryWishList"><button type="submit" class="btn btn-primary button-travel buttonMargin darkmode-ignore"
        name="countryWishList" id="countryWishList">Add to my wishlist</button></a>
    <a href="/country/{{data._id}}/countryVisitor"><button type="submit" class="btn btn-primary button-travel buttonMargin darkmode-ignore"
        name="countryVisitor" id="countryVisitor">Add to my visited list</button></a>
</div>
